# Set personal aliases, overriding those provided by oh-my-zsh libs,
# plugins, and themes. Aliases can be placed here, though oh-my-zsh
# users are encouraged to define aliases within the ZSH_CUSTOM folder.
# For a full list of active aliases, run `alias`.
#
# Example aliases
alias zshconfig="code ~/.zshrc"
alias cpwd="pwd|pbcopy"
alias jsons="json-server --watch"
alias bnode="babel-node --extensions .ts,.js"
alias edev="export AWS_PROFILE=dev"
alias eqa="export AWS_PROFILE=qa"
alias estaging="export AWS_PROFILE=staging"
alias eprod="export AWS_PROFILE=prod"
alias eray="export AWS_PROFILE=ray"
alias openvp="code /Users/raynaldo/VitusVet/VitusPay/frontend/vitusvet-payments-portal && code /Users/raynaldo/VitusVet/VitusPay/backend/VitusPay"
alias openvsrx="code /Users/raynaldo/VitusVet/Pharma/frontend/Pharma-Web && code /Users/raynaldo/VitusVet/Pharma/backend/Pharma-Serverless"
alias tagqadev="npm run tag:dev && npm run tag:qa"
alias awsprofile= "code ~/.aws/credentials"
alias awsconfig= "code ~/.aws/config"

# fnm
export PATH=/Users/Raynaldo/.fnm:$PATH
eval "$(fnm env --use-on-cd)"

# ruby and chruby
source /opt/homebrew/opt/chruby/share/chruby/chruby.sh
source /opt/homebrew/opt/chruby/share/chruby/auto.sh
chruby ruby-3.2.2

